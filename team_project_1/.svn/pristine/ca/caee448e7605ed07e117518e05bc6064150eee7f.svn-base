package xyz.for01.reservation;

import java.sql.Date;

public class ReservationVo {
	private String name;
	private String phone;
	private int grade;
	private Date res_start;
	private Date res_end;
	private int hotelNo;
	private int roomNo;
	private String price;
	private Date checkIn;
	private Date checkOut;
	private Date resDate;
	private int resStat;
	
	public ReservationVo() { }

	public ReservationVo(String name, String phone, int grade, Date res_start, Date res_end, int hotelNo, int roomNo,
			String price, Date checkIn, Date checkOut, Date resDate, int resStat) {
		super();
		this.name = name;
		this.phone = phone;
		this.grade = grade;
		this.res_start = res_start;
		this.res_end = res_end;
		this.hotelNo = hotelNo;
		this.roomNo = roomNo;
		this.price = price;
		this.checkIn = checkIn;
		this.checkOut = checkOut;
		this.resDate = resDate;
		this.resStat = resStat;
	}

	public ReservationVo(String name, String phone, int grade, Date res_start, Date res_end, int hotelNo, int roomNo,
			String price) {
		super();
		this.name = name;
		this.phone = phone;
		this.grade = grade;
		this.res_start = res_start;
		this.res_end = res_end;
		this.hotelNo = hotelNo;
		this.roomNo = roomNo;
		this.price = price;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public int getGrade() {
		return grade;
	}

	public void setGrade(int grade) {
		this.grade = grade;
	}

	public Date getRes_start() {
		return res_start;
	}

	public void setRes_start(Date res_start) {
		this.res_start = res_start;
	}

	public Date getRes_end() {
		return res_end;
	}

	public void setRes_end(Date res_end) {
		this.res_end = res_end;
	}

	public int getHotelNo() {
		return hotelNo;
	}

	public void setHotelNo(int hotelNo) {
		this.hotelNo = hotelNo;
	}

	public int getRoomNo() {
		return roomNo;
	}

	public void setRoomNo(int roomNo) {
		this.roomNo = roomNo;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public Date getCheckIn() {
		return checkIn;
	}

	public void setCheckIn(Date checkIn) {
		this.checkIn = checkIn;
	}

	public Date getCheckOut() {
		return checkOut;
	}

	public void setCheckOut(Date checkOut) {
		this.checkOut = checkOut;
	}

	public Date getResDate() {
		return resDate;
	}

	public void setResDate(Date resDate) {
		this.resDate = resDate;
	}

	public int getResStat() {
		return resStat;
	}

	public void setResStat(int resStat) {
		this.resStat = resStat;
	}
	
}
