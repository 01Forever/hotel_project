package xyz.for01.boardvo;

import java.util.Date;
import java.util.List;

import com.sun.javafx.collections.MappingChange.Map;

public class BoardVo {
	private int boardno;
	private int category_num;
	private String emailid;
	private String title;
	private String content;
	private Date writedate;
	private List<BoardFileVo> files;
	



	public List<BoardFileVo> getFiles() {
		return files;
	}


	public void setFiles(List<BoardFileVo> files) {
		this.files = files;
	}


	public BoardVo() { }


	public BoardVo(int boardno, int category_num, String emailid, String title, String content, Date writedate) {
		super();
		this.boardno = boardno;
		this.category_num = category_num;
		this.emailid = emailid;
		this.title = title;
		this.content = content;
		this.writedate = writedate;
	}


	public int getBoardno() {
		return boardno;
	}


	public void setBoardno(int boardno) {
		this.boardno = boardno;
	}


	public int getCategory_num() {
		return category_num;
	}


	public void setCategory_num(int category_num) {
		this.category_num = category_num;
	}


	public String getEmailid() {
		return emailid;
	}


	public void setEmailid(String emailid) {
		this.emailid = emailid;
	}


	public String getTitle() {
		return title;
	}


	public void setTitle(String title) {
		this.title = title;
	}


	public String getContent() {
		return content;
	}


	public void setContent(String content) {
		this.content = content;
	}


	public Date getWritedate() {
		return writedate;
	}


	public void setWritedate(Date writedate) {
		this.writedate = writedate;
	}


	@Override
	public String toString() {
		return "BoardVo [boardno=" + boardno + ", category_num=" + category_num + ", emailid=" + emailid + ", title="
				+ title + ", content=" + content + ", writedate=" + writedate + ", files=" + files + "]";
	}
	


}