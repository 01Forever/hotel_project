package xyz.for01.membervo;

import java.sql.Timestamp;

public class MemberVo {
	private String emailid;
	private String pwd;
	private String nic;
	private String phone;
	private Timestamp joindate;
	private String grade;
	private String delyn;
	private int userno;
	private long mileage;
	
	public MemberVo() {}
	
	public MemberVo(String emailid, String pwd, String nic, String phone, String grade, String delyn, int userno) {
		this.emailid = emailid;
		this.pwd = pwd;
		this.nic = nic;
		this.phone = phone;
		this.grade = grade;
		this.delyn = delyn;
		this.userno = userno;
	}
	
	public MemberVo(String emailid, String nic, String phone, String grade, int userno, long mileage) {
		super();
		this.emailid = emailid;
		this.nic = nic;
		this.phone = phone;
		this.grade = grade;
		this.userno = userno;
		this.mileage = mileage;
	}

	public String getEmailid() {
		return emailid;
	}
	public void setEmailid(String emailid) {
		this.emailid = emailid;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
	public String getNic() {
		return nic;
	}
	public void setNic(String nic) {
		this.nic = nic;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public Timestamp getJoindate() {
		return joindate;
	}
	public void setJoindate(Timestamp joindate) {
		this.joindate = joindate;
	}
	public String getGrade() {
		return grade;
	}
	public void setGrade(String grade) {
		this.grade = grade;
	}

	public String getDelyn() {
		return delyn;
	}

	public void setDelyn(String delyn) {
		this.delyn = delyn;
	}

	
	public void setUserno(int userno) {
		this.userno = userno;
	}

	public int getUserno() {
		return userno;
	}

	public long getMileage() {
		return mileage;
	}

	public void setMileage(long mileage) {
		this.mileage = mileage;
	}

	@Override
	public String toString() {
		return "MemberVo [emailid=" + emailid + ", pwd=" + pwd + ", nic=" + nic + ", phone=" + phone + ", joindate="
				+ joindate + ", grade=" + grade + ", delyn=" + delyn + ", userno=" + userno + ", mileage=" + mileage
				+ "]";
	}

}
