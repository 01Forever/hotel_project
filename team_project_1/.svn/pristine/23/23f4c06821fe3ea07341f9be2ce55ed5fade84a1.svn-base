package xyz.for01.hotelvo;

import java.util.List;

public class RoomVo {
	private int hotelNo;
	private String hotelName;
	private int roomNo;
	private String roomName;
	private String price;
	private String content;
	private int maxRoom;
	private int resCount;
	
	private List<RoomImgVo> imgs;
	
	public RoomVo() {}

	public RoomVo(int roomNo, String roomName, String price, String content, List<RoomImgVo> imgs) {
		super();
		this.roomNo = roomNo;
		this.roomName = roomName;
		this.price = price;
		this.content = content;
		this.imgs = imgs;
	}

	public RoomVo(int hotelNo, String hotelName, int roomNo, String roomName, String price) {
		this.hotelNo = hotelNo;
		this.hotelName = hotelName;
		this.roomNo = roomNo;
		this.roomName = roomName;
		this.price = price;
	}

	
	public RoomVo(int roomNo, String roomName, String price, String content, int maxRoom,
			int resCount, List<RoomImgVo> imgs) {
		this.roomNo = roomNo;
		this.roomName = roomName;
		this.price = price;
		this.content = content;
		this.maxRoom = maxRoom;
		this.resCount = resCount;
		this.imgs = imgs;
	}

	public int getRoomNo() {
		return roomNo;
	}

	public void setRoomNo(int roomNo) {
		this.roomNo = roomNo;
	}

	public String getRoomName() {
		return roomName;
	}

	public void setRoomName(String roomName) {
		this.roomName = roomName;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public List<RoomImgVo> getImgs() {
		return imgs;
	}

	public void setImgs(List<RoomImgVo> imgs) {
		this.imgs = imgs;
	}

	public int getHotelNo() {
		return hotelNo;
	}

	public void setHotelNo(int hotelNo) {
		this.hotelNo = hotelNo;
	}

	public String getHotelName() {
		return hotelName;
	}

	public void setHotelName(String hotelName) {
		this.hotelName = hotelName;
	}

	public int getMaxRoom() {
		return maxRoom;
	}

	public void setMaxRoom(int maxRoom) {
		this.maxRoom = maxRoom;
	}

	public int getResCount() {
		return resCount;
	}

	public void setResCount(int resCount) {
		this.resCount = resCount;
	}
	
}
