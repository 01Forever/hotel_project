@charset "UTF-8";

#wrap {
	width: 971px;
	margin: 0;
	margin-left: auto;
	margin-right: auto;
}
h1 {
	color:  #f7323f;
}
table {
	whidth : 100%;
	border-collapse: collapse;
	font-size: 12px;
	line-height: 24px;
}
table td, th{
	border: #d3d3d3 solid 1px;
	padding: 5px;
}
th {
	size=20;
	background: #f7323f;
}
img {
	width: 220px;
	height: 300px;
}
a {
	text-decoration: none;
	color: black;
}
a:HOVER{
	text-decoration: underline;
	color:  #f7323f;
}